page {
    background-color: #fff;
}
.msg-item {
    display: flex;
    justify-content: flex-start;
    align-items: flex-start;
    position: relative;
}

.avatar {
    order: 1;
    width: 85rpx;
    height: 85rpx;
    padding: 20rpx;
    display: inline-block;
}

.message {
    order: 3;
    min-height: 100rpx;
    max-width: 500rpx;
    word-break: break-all;
    padding: 20rpx;
    margin-top: 15rpx;
    border-radius: 10rpx;
    background-color: #41d1ad;
}
.triangle {
    order: 2;
    border-top: 8px solid transparent;
    border-bottom: 8px solid transparent;
    border-right: 12px solid #41d1ad;
    margin-top: 42rpx;
    width: 0;
    height: 0;

}
.play-voice-second {
    order: 4;
    padding-top: 30rpx;
    width: 70rpx;
    height: 76rpx;
    line-height: 60rpx;
    text-align: center;
    font-size: 14px;
    color: #ccc;
}

.msg-item.isme {
    justify-content: flex-end;
}
.msg-item.isme .avatar {
    order: 4;
}
.msg-item.isme .message {
    order: 2;
}
.msg-item.isme .triangle {
    order: 3;
    border-right: none;
    border-left: 12px solid #41d1ad;
}
.msg-item.isme .play-voice-second {
    order: 1;
    left: -100rpx;
}
.msg-item.isme .msg-voice {
    text-align: right;
}
.msg-item.isme .msg-voice .play-voice-icon {
    -webkit-transform: rotate(180deg);
    transform: rotate(180deg);
}


.elsePanel {
    height: 150px;
    background-color: #fff;
}

.else-row {
    line-height: 40rpx;
}
.moreImage{
    width: 40px;
    height: 40px;
    padding: 8px;
    border: 1px solid #e3e3e3;
    border-radius: 10px;
    background: #f3f3f3;
    margin: 5px;
}
.emotionPanel {
    padding: 20rpx;
    height: 150px;
    flex-wrap: wrap;
    display: flex;
    background-color: #fff;
}
.emotion {
    height: 60rpx;
    width: 60rpx;
    margin: 10rpx;
}
.map-box {
    height: 100%;
    width: 100%;
    position: absolute;
    top: 0;
    left: 0;
    z-index: 110;
    background-color: rgba(0, 0, 0, 0.6)
}
.map-box .map-wrap {
    position: absolute;
    height: 100%;
    width: 100%;
    z-index: 1;
    top: 0;
}
.map-box .close {
    position: absolute;
    bottom: 0;
    right: 0;
    z-index: 11;
    padding: 10rpx;
    background-color: transparent;
}

.large-wrap {
    position: fixed;
    top: 0;
    z-index: 10;
    height: 100%;
    width: 100%;
    background-color: rgba(0, 0, 0, 0.7);
    -webkit-transition: all 1s;
    transition: all 1s;
}
.large-wrap .large-image {
    position: absolute;
    width: 100%;
    top: 50%;
    -webkit-transform: translateY(-50%);
    transform: translateY(-50%);
}
.large-wrap .close-btn {
    position: absolute;
    z-index: 11;
    top: 40rpx;
    right: 40rpx;
}

.wa-row {
    display: flex;
    align-items: center;
    height: 50px;
    padding: 10rpx;
    background-color: #fff;
}

.input-box , .voice-box {
    flex: 1;
    height: 80rpx;
    padding: 15rpx;
    margin: 4rpx 12rpx;
    border-bottom: 1px solid #4a4949;
}
.voice-box {
    padding: 10rpx 20rpx;
    border: none;
}
.input-box input {
    height: 50rpx;
    line-height: 50rpx;
    font-size: 14px;
}

.voice-box .voice-btn {
    color: #4a4949;
    border: 1px solid #4a4949;
    border-radius: 14rpx;
    text-align: center;
    line-height: 64rpx;
    margin: 0 10rpx;
}
.voice-btn.keypress {
    background-color: rgba(221, 221, 221, .8);
}
.record-voice-pop {
    position: fixed;
    height: 400rpx;
    width: 400rpx;
    z-index: 100;
    top: 50%;
    left: 50%;
    margin: -200rpx 0 0 -200rpx;
    border-radius: 30rpx;
    background-color: rgba(0, 0, 0, 0.6);
}
.record-voice-pop .record-voice {
    position: absolute;
    top: 100rpx;
    left: 50%;
    margin-left: -50rpx;
    width: 100rpx;
    height: 144rpx;
    animation: voice-play2 1s infinite steps(4, end);
    /*background-image: url('/media/voice3.png');*/
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: 0 0;
    background-size: 400rpx 144rpx;
}
.record-voice-pop .record-text {
    position: absolute;
    bottom: 20rpx;
    left: 0;
    width: 100%;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    color: #fff;
}
.record-voice-pop .record-second {
    position: absolute;
    top: 20rpx;
    right: 0;
    width: 100rpx;
    height: 60rpx;
    line-height: 60rpx;
    text-align: center;
    color: #fff;
}
.msg-voice {
    height: 60rpx;
    padding: 10rpx 20rpx;
    text-align: left;
    min-width: 75px;
    background-color: #48cc83;
    border-radius: 12rpx;
    box-shadow: 1px 1px 5px rgba(0, 0, 0, 0.5);
}

.play-voice-icon {
    display: inline-block;
    /*margin-left: 10rpx;*/
    width: 30rpx;
    height: 40rpx;
    background-image: url('data:image/png;base64,');
    background-repeat: no-repeat;
    background-position: 0 -80rpx;
    background-size: 30rpx 120rpx;
}
.msg-voice .play-voice-icon.play {
    animation: voice-play 1s infinite steps(3, end);
    background-position: 0 0;
}

/*声音录音动画*/
@-webkit-keyframes voice-play2 {
    100% {
        background-position-x: -400rpx;
    }
}
@keyframes voice-play2 {
    100% {
        background-position-x: -400rpx;
    }
}
/*声音播放动画*/
@-webkit-keyframes voice-play {
    100% {
        background-position-y: -120rpx;
    }
}
@keyframes voice-play {
    100% {
        background-position-y: -120rpx;
    }
}


.icon {
    vertical-align: middle;
    margin-top: 8rpx;
    width: 64rpx;
    height: 64rpx;
}
