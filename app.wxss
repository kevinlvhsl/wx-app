/**app.wxss**/
html, body, view, text, input, textarea, scroll-view {
  -webkit-box-sizing: border-box;
  box-sizing: border-box;
}
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 100rpx 0;
  box-sizing: border-box;
}

page {
  height: 100%;
  background-color: #d9e5db;
  overflow-y: auto;
}

.flex {
  display: flex;
}
